	<script type="text/javascript">
	//<![CDATA[
			function stopRKey(evt) {
			var evt = (evt) ? evt : ((event) ? event : null);
			var node = (evt.target) ? evt.target : ((evt.srcElement) ? evt.srcElement : null);
			if ((evt.keyCode == 13) && (node.type=="text"))  {return false;}
		}
	
		document.onkeypress = stopRKey;

		function disable_f5(e)
		{
			if ((e.which || e.keyCode) == 116)
			{
				alert('Attenzione: questo sistema NON consente\nil refresh della pagina tramite il tasto F5');
				e.preventDefault();
			}
		}

		$(document).ready(function(){
			$(document).bind("keydown", disable_f5);
		});
	function changePrintLayout(layout)
	{
		var xmlHttp;

		if (window.XMLHttpRequest)     // Firefox, Safari, ...
			xmlHttp = new XMLHttpRequest();
		else if (window.ActiveXObject)   // ActiveX version
			xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");    // Internet Explorer

		
		var url = "printChangeLayout.php?id="+layout;
		xmlHttp.open("GET", url, true);
		xmlHttp.onreadystatechange = function()
		{
			if (xmlHttp.readyState == 4)
			{
				window.location = '';
			}
		};
		xmlHttp.send(null);
	}

	var open_info = '-';
	var open_op = '-';
	var effect = 'appear';
	function printPopup(idEntry, h, which_popup, grid_type, page_element, align)
	{
		id = which_popup+"_"+idEntry+"_"+h;
		closeOpenPopup(open_op);
		if (id == open_info)
		{
			if(align == 'SX')
				showPopupAlignSX(which_popup, idEntry, h, effect);
			else
				showPopupAlignDX(which_popup, idEntry, h, effect);
		}
		else
		{
			var xmlHttp;
			
			if (window.XMLHttpRequest)     // Firefox, Safari, ...
				xmlHttp = new XMLHttpRequest();
			else if (window.ActiveXObject)   // ActiveX version
				xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");    // Internet Explorer
		
			var url = "printPopup.php";
			var params = "id="+idEntry+"&h="+h+"&which="+which_popup+"&vista="+grid_type+"&_lang=it&location_pathname="+encodeURIComponent(location.pathname)+"&location_search="+encodeURIComponent(location.search);

			xmlHttp.open("GET", url+"?"+params, true);
			/*xmlHttp.open("POST", url, true);
			xmlHttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			xmlHttp.setRequestHeader("Content-length", params.length);
			xmlHttp.setRequestHeader("Connection", "close");*/

			xmlHttp.onreadystatechange = function()
			{
				if (xmlHttp.readyState == 4)
				{
					var operations_html = xmlHttp.responseText;
					document.getElementById(page_element).innerHTML = operations_html;
					// if(which_popup == 'id_agenda')
						// showPopupAlignSX(which_popup, idEntry, h, effect);
					// else
						// showPopupAlignDX(which_popup, idEntry, h, effect);
					if(align == 'SX')
						showPopupAlignSX(which_popup, idEntry, h, effect);
					else
						showPopupAlignDX(which_popup, idEntry, h, effect);
					open_info = id;
				}
			};
			xmlHttp.send(null); // POST: params);
		}
	}

	function syncronizePopup(prefix, i, h, eff)
	{
		id = prefix+"_"+i+"_"+h;

		closeOpenPopup(open_info);
		if (id == open_op)
			showPopupAlignDX(prefix, i, h, eff);
		else
		{
			closeOpenPopup(open_op);
			showPopupAlignDX(prefix, i, h, eff);
		}

		open_op = id;
	}

	function closeOpenPopup(open_box)
	{
		if (open_box != '-')
			document.getElementById(open_box).style.display = 'none';
	}

	function showDynamicEventPopup(which_popup, tableID, i, h, effect, link, page_element)
	{
		id = which_popup+"_"+tableID+"_"+i+"_"+h;
		closeOpenPopup(open_op);
		if (id == open_info)
			showEventPopup(which_popup, tableID, i, h, effect);
		else
		{
			var xmlHttp;
				
			if (window.XMLHttpRequest)     // Firefox, Safari, ...
				xmlHttp = new XMLHttpRequest();
			else if (window.ActiveXObject)   // ActiveX version
				xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");    // Internet Explorer
	
			var url = "printPopup.php?id="+i+"&h="+h+"&which="+which_popup+"&vista="+tableID+"&url="+link+"&_lang=it";
			xmlHttp.open("GET", url, true);
			xmlHttp.onreadystatechange = function()
			{
				if (xmlHttp.readyState == 4)
				{
					var operations_html = xmlHttp.responseText;
			
					document.getElementById(page_element).innerHTML = operations_html;
					showEventPopup(which_popup, tableID, i, h, effect)
					open_info = id;
				}
			};
			xmlHttp.send(null);
		}
	}
	
	function reservationQuickAdd()
	{
		var xmlHttp;

		if (window.XMLHttpRequest)     // Firefox, Safari, ...
			xmlHttp = new XMLHttpRequest();
		else if (window.ActiveXObject)   // ActiveX version
			xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");    // Internet Explorer

		reservation_entry_type = document.getElementById('quick[entry_type]').value;
		if (reservation_entry_type == 0)
		{
			alert('Seleziona il tipo di prenotazione');
			return;
		}
		
		reservation_title = document.getElementById('quick[name]').value;
		if (reservation_title == '')
		{
			alert('Inserisci un titolo per la prenotazione');
			return;
		}

		reservation_room = document.getElementById('quick[room]').value;
		reservation_day = document.getElementById('quick[day]').innerHTML;
		reservation_from = document.getElementById('quick[from]').value;
		reservation_to = document.getElementById('quick[to]').value;
		//alert(reservation_entry_type+" "+reservation_title+" "+reservation_room+" "+reservation_day+" "+reservation_from+" "+reservation_to);

		var url = "printQuickAdd.php?";
		url += "entry_type="+reservation_entry_type;
		url += "&name="+reservation_title;
		url += "&room="+reservation_room;
		url += "&day="+reservation_day;
		url += "&from_time="+reservation_from;
		url += "&to_time="+reservation_to;
		url += "&_lang=it";
		
		xmlHttp.open("GET", url, true);
		xmlHttp.onreadystatechange = function()
		{
			if (xmlHttp.readyState == 4)
			{
				//var exit_status = xmlHttp.responseText;
				xmlDoc = xmlHttp.responseXML;
				var exit_status = xmlDoc.getElementsByTagName('ExitStatus')[0].firstChild.nodeValue;
				if (exit_status == 0)
				{
					var message = xmlDoc.getElementsByTagName('Message')[0].firstChild.nodeValue;
					alert(message);
				}
				else
				{
					var message = xmlDoc.getElementsByTagName('Message')[0].firstChild.nodeValue;
					alert(message);
					window.location = '/Aule//index.php?content=gestore_spazi&area=27&day=07&month=02&year=2018&from=07:30:00&to=21:30:00&vista=day&length=30&minCapacity=8&maxCapacity=240';
				}
			}
		};
		xmlHttp.send(null);
	}

	function reservationQuickEdit()
	{
		var xmlHttp;

		if (window.XMLHttpRequest)     // Firefox, Safari, ...
			xmlHttp = new XMLHttpRequest();
		else if (window.ActiveXObject)   // ActiveX version
			xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");    // Internet Explorer

		reservation_id = document.getElementById('quick[entry_id]').value;
		reservation_entry_type = document.getElementById('quick[entry_type]').value;
		if (reservation_entry_type == 0)
		{
			alert('Seleziona il tipo di prenotazione');
			return;
		}

		reservation_title = document.getElementById('quick[name]').value;
		if (reservation_title == '')
		{
			alert('Inserisci un titolo per la prenotazione');
			return;
		}

		reservation_room = document.getElementById('quick[room]').value;
		reservation_day = document.getElementById('quick[day]').innerHTML;
		reservation_from = document.getElementById('quick[from]').value;
		reservation_to = document.getElementById('quick[to]').value;
		//alert(reservation_id+" "+reservation_entry_type+" "+reservation_title+" "+reservation_room+" "+reservation_day+" "+reservation_from+" "+reservation_to);

		var url = "printQuickEdit.php?";
		url += "id="+reservation_id;
		url += "&entry_type="+reservation_entry_type;
		url += "&name="+reservation_title;
		url += "&room="+reservation_room;
		url += "&day="+reservation_day;
		url += "&from_time="+reservation_from;
		url += "&to_time="+reservation_to;
		url += "&_lang=it";
		
		xmlHttp.open("GET", url, true);
		xmlHttp.onreadystatechange = function()
		{
			if (xmlHttp.readyState == 4)
			{
				//var exit_status = xmlHttp.responseText;
				xmlDoc = xmlHttp.responseXML;
				var exit_status = xmlDoc.getElementsByTagName('ExitStatus')[0].firstChild.nodeValue;
				if (exit_status == 0)
				{
					var message = xmlDoc.getElementsByTagName('Message')[0].firstChild.nodeValue;
					alert(message);
				}
				else
				{
					var message = xmlDoc.getElementsByTagName('Message')[0].firstChild.nodeValue;
					alert(message);
					window.location = '/Aule//index.php?content=gestore_spazi&area=27&day=07&month=02&year=2018&from=07:30:00&to=21:30:00&vista=day&length=30&minCapacity=8&maxCapacity=240';
				}
			}
		};
		xmlHttp.send(null);
	}

	function manageNotes(op_type)
	{
		var xmlHttp;

		if (window.XMLHttpRequest)     // Firefox, Safari, ...
			xmlHttp = new XMLHttpRequest();
		else if (window.ActiveXObject)   // ActiveX version
			xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");    // Internet Explorer

		reservation_id = document.getElementById('manage_notes[entry_id]').value;

		reservation_notes = document.getElementById('manage_notes[text]').value;
		
		var url = "manageNotes.php?";
		url += "id="+reservation_id;
		url += "&operation="+op_type;
		url += "&notes="+reservation_notes.replace(/\n/g, "<br/>");
		url += "&_lang=it";
		
		xmlHttp.open("GET", url, true);
		xmlHttp.onreadystatechange = function()
		{
			if (xmlHttp.readyState == 4)
			{
				//var exit_status = xmlHttp.responseText;
				xmlDoc = xmlHttp.responseXML;
				var exit_status = xmlDoc.getElementsByTagName('ExitStatus')[0].firstChild.nodeValue;
				if (exit_status == 0)
				{
					var message = xmlDoc.getElementsByTagName('Message')[0].firstChild.nodeValue;
					alert(message);
				}
				else
				{
					var message = xmlDoc.getElementsByTagName('Message')[0].firstChild.nodeValue;
					alert(message);
					window.location = '/Aule//index.php?content=gestore_spazi&area=27&day=07&month=02&year=2018&from=07:30:00&to=21:30:00&vista=day&length=30&minCapacity=8&maxCapacity=240';
				}
			}
		};
		xmlHttp.send(null);
	}

	<!-- script altezza iframe-->

	  var loaded = false;
	  function iframeLoaded() {
		  var iFrameID = document.getElementById('listafile');
		  var container = document.getElementById('helpSelector');
		  if(iFrameID && loaded) {
				iFrameID.width = container.offsetWidth + "px";
				iFrameID.height = "";
				iFrameID.height = iFrameID.contentWindow.document.body.scrollHeight + "px";
				$("#helpLoad").css({"display": "none"});
		  }   
	  }
	<!-- jQuery comparsa popup help -->
		function mostraPopup(id){
				$("#black").css({
					"visibility": "visible",
					"opacity": "0.5",
					"z-index": "3"});
				$(id).css({
					"visibility": "visible",
					"opacity": "1",
					"z-index": "3"});
		}
		function nascondiPopup(){
				$("#black").css({
					"visibility": "hidden",
					"opacity": "0",
					"z-index": "-1"});
				$("#helpSelector").css({
					"visibility": "hidden",
					"opacity": "0",
					"z-index": "-1"});
				$("#bloccoPopup").css({
					"visibility": "hidden",
					"opacity": "0",
					"z-index": "-1"});
				$("#cercaParola").css({
					"visibility": "hidden",
					"opacity": "0",
					"z-index": "-1"});
		}
		//carica iframe
		function loadIframe(){
			$('#listafile').attr('src','linguettaHelp.php?_lang=it');
			loaded = true;
		}
		//controllo popup blocker
		if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) === false) { //controllo mobile
			if (document.cookie.replace(/(?:(?:^|.*;\s*)popupBlock\s*\=\s*([^;]*).*$)|^.*$/, "$1") !== "true") { //controllo cookie
				 setTimeout( function() {
					var popup = window.open("popupcheck.html","controllo popup","width=100, height=100",false);
					if(!popup || popup.outerHeight == 0) {
						//alert("Blocco popup attivato! Aggiungi il sito alla lista delle eccezioni");
						mostraPopup("#bloccoPopup");
					} else {
						//chiudi finestra
						popup.close();
						//imposta valore cookie 
						document.cookie = "popupBlock=true; expires=Fri, 31 Dec 9999 23:59:59 GMT; path=/";
					} 
				}, 25);
			}
		}
		
	//]]>
	</script>
</head>


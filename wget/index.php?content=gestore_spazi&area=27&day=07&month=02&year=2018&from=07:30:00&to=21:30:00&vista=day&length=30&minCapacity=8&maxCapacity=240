							<script type="text/javascript">
							function RedirectRicerca(keyword){
																window.location.replace('/Aule//index.php?content=gestore_report&DataInizio=1518044400&DataFine=1577833199&cercaRipetizione=-1&cercaChiave'+'='+keyword);
							}
							
							function gestisciSearchBox(){
								if($('#search').is(":hidden")){
									//mostra textbox ricercate
									$('#search').fadeIn(800);
								}else{
									//genera testo e mostra popup
									generaTesto();
								}
							}
							
							function generaTesto(){
								var keyword = $('#search').val();
								if(keyword.length <3){
									$('#cercaParola span').text("La parola chiave deve contenere almeno 3 caratteri!");
									$("#btnCerca").hide();
								}else{
									// keyword= "<span style='color:#FF0000; font-size:15px;'>'"+keyword+"'</span>";
									keyword= "<span style='font-weight: bold; font-style: italic; color: #000000; font-size:15px;'>'"+keyword+"'</span>";
									$("#btnCerca").show();
																		// $('#cercaParola span').html("Verranno ricercate le prenotazioni che contengono nel titolo, nella descrizione o nel docente la parola chiave "+keyword+" nel periodo 08/02/2018 - 31/12/2019");
									$('#cercaParola span').html("Ricerca delle prenotazioni che contengono la parola chiave<br/> "+keyword+" <br/>nelle sezioni Codice prenotazione, Titolo prenotazione, Descrizione completa e Utilizzatori<br/>nel periodo <span style='font-weight: bold; font-style: italic; color: #000000; font-size:15px;'>08/02/2018 - 31/12/2019</span>");
								}
								mostraPopup('#cercaParola');
							}
							
							function intercettaInvio(event){
								var k = event.which || event.keyCode;
								if (k == 13) //se il tasto premuto Ã¨ invio
									generaTesto();
							}
							</script>
							<div id="cercaParola" class="popupmsg">
								<span id="testoRicerca"></span>
								<br/>
								<input type="button" name="annulla" id="btnAnnulla" style="margin-top: 5px;" value="Annulla" onclick="nascondiPopup();" />
								<input type="button" name="search" id="btnCerca" value="Cerca" onclick="RedirectRicerca($('#search').val());" />
							</div>
							<input type="text" class="txtRicerca" id="search" placeholder="Cerca" onkeypress="intercettaInvio(event);"/>
							<a href="#" onclick="gestisciSearchBox();" class="imgRicerca"><img src="images/general_search.png" /></a>

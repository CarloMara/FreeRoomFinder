<script type="text/javascript">
//<![CDATA[

function download_script(nome, path, flag)
{
	var browser=navigator.appName;
	var b_version=navigator.appVersion;
	var u_agent= navigator.userAgent;

	var indexV = b_version.indexOf('MSIE 7');
	var indexA = u_agent.indexOf('MSIE 7');

	var lang = '';
	if(indexV != -1 || indexA != -1){
		if(lang)
			link_name= 'link_download.php?nameFile='+nome+'&pathFile='+path+'&deleteFile='+flag+'&_lang='+lang;
		else
			link_name= 'link_download.php?nameFile='+nome+'&pathFile='+path+'&deleteFile='+flag;

		window.open(link_name, null, 'height=200,width=400');
	}
	else{
		link_name= 'download_file.php?nameFile='+nome+'&pathFile='+path+'&deleteFile='+flag;
		window.open(link_name);
	}

	return true;
}

function checkDate(form)
{
	
	var dataInizio	= (document.getElementById('DataInizio').value).split("-");
	var dataFine	= (document.getElementById('DataFine').value).split("-");
	
	// if((! isCorrectDate(form.elements['DataInizio[d]'].value, form.elements['DataInizio[M]'].value, form.elements['DataInizio[Y]'].value)) || (! isCorrectDate(form.elements['DataFine[d]'].value, form.elements['DataFine[M]'].value, form.elements['DataFine[Y]'].value)) )
	if((! isCorrectDate(dataInizio[2], dataInizio[1], dataInizio[0])) || (! isCorrectDate(dataFine[2], dataFine[1], dataFine[0])) )
	{
		msg =	"Il formato della data non è valido:" + "\n" +
				" - controlla il numero di giorni del mese selezionato" + "\n" +
				" - controlla gli anni bisestili";

		alert(msg);
		return false;
	}
	
	// DateS= new Date(form.elements['DataInizio[Y]'].value, form.elements['DataInizio[M]'].value, form.elements['DataInizio[d]'].value);
	// DateE= new Date(form.elements['DataFine[Y]'].value, form.elements['DataFine[M]'].value, form.elements['DataFine[d]'].value);
	DateS= new Date(dataInizio[0], parseInt(dataInizio[1])-1, dataInizio[2]);
	DateE= new Date(dataFine[0], parseInt(dataFine[1])-1, dataFine[2]);

	if(DateS > DateE){
		msg = "La data di inzio deve essere inferiore alla data di fine.";
		alert(msg);
		return false;
	}
	//aule occupate
	//se è settato "visualizza aule occupate" ed è stata inserita una durata minima, avviso che questa non verrà considerata
	if(form.elements['AuleOccupate'].checked && form.elements['Durata'].value!=""){
		msg = "Con la spunta su Visualizza aule occupate non viene considerato il parametro Durata minima.";
		alert(msg);
	}

	return true;
}

function addHiddenInput(input_name, val, form){
	var input = document.createElement('input');
	input.type = 'hidden';
	input.name = input_name;
    input.value = val;
    form.appendChild(input);
}


</script>


